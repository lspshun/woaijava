package 控制语句;
public class PrintNum {
    public static void main(String[] args){
         // 外层循环控制行数
        for (int i = 1; i<=5; i++) {

            // 内层循环控制每行的数
            // 内层循环变量的最大值和外层循环变量的值相等
            for (int j = 1; j<=i; j++) {

                System.out.print(j);
            }

            // 每打印完一行后进行换行
            System.out.println();
        }
    }
}