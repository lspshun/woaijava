package 方法;

public class Can {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Can a = new Can();
		double rAvg;
		int sub1 = 78;
		int sub2 = 99;
		rAvg = a.calcAve(sub1,sub2);
		System.out.println("平均分"+rAvg);
		

	}

	private double calcAve(int sub1, int sub2) {
		// TODO Auto-generated method stub
		double rAve = (sub1+sub2)/2;
		return rAve;
	}

}
