package 控制语句;

public class PrintNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		外层循环函数
		for (int i = 0; i < args.length; i++) {
//			内层循环控制每行的数
//			内层循环变量的最大值和外层循环变量的值相等
			for (int j = 1; j <= i; j++) {
				System.out.print(j);
				
			}
//			每打印一行后进行换行
			System.out.println();
		}
	}

}
